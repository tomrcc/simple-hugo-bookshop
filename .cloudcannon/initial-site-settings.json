{
	"ssg": "hugo",
	"install_command": "echo 'HUGO VERSION' && hugo version && git submodule update --init --recursive",
	"environment": "",
	"source": "",
	"destination": "public",
	"baseURL": "https://www.antwerpchocolate.com",
	"config": "hugo.toml",
	"configDir": "",
	"contentDir": "",
	"layoutDir": "",
	"themesDir": "",
	"theme": "",
	"logFile": "",
	"ignoreVendorPaths": "",
	"disableKinds": "",
	"buildDrafts": false,
	"buildExpired": false,
	"buildFuture": false,
	"cleanDestinationDir": false,
	"debug": false,
	"enableGitInfo": false,
	"gc": false,
	"i18nWarnings": false,
	"ignoreCache": false,
	"ignoreVendor": false,
	"log": false,
	"minify": false,
	"noChmod": false,
	"noTimes": false,
	"pathWarnings": false,
	"mem": false,
	"quiet": false,
	"templateMetrics": false,
	"templateMetricsHints": false,
	"verbose": false,
	"verboseLog": false,
	"preserved_paths": "node_modules/,.hugo_cache/,resources/",
	"preserveOutput": false,
	"includeGit": true,
	"use_beta_plugin": false,
	"environment_variables": [
		{
			"key": "HUGO_CACHEDIR",
			"value": "/usr/local/__site/src/.hugo_cache/"
		}
	]
}